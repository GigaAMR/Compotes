{
	"name": "@orbitale/compotes-app",
	"version": "1.0.0",
	"private": true,
	"license": "LGPL-3.0-or-later",
	"scripts": {
		"app": "concurrently --names \"front,back\" --raw \"vite dev\" \"tauri dev\"",
		"dev": "vite dev",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. src && eslint --ignore-path .gitignore src",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. src",
		"test": "yarn --cwd tests/webdriverio run test",
		"tauri": "tauri",
		"postinstall": "yarn --cwd tests/webdriverio/"
	},
	"devDependencies": {
		"@popperjs/core": "^2.11.6",
		"@sveltejs/adapter-static": "1.0.6",
		"@sveltejs/kit": "^1.3.10",
		"@tauri-apps/api": "^1.2.0",
		"@tauri-apps/cli": "^1.2.3",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"@zerodevx/svelte-toast": "^0.8.2",
		"bootstrap": "^5.2.3",
		"chart.js": "^4.2.0",
		"concurrently": "^7.6.0",
		"cross-os": "^1.5.0",
		"date-fns": "^2.29.3",
		"date-picker-svelte": "^2.2.5",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"luxon": "^3.2.1",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"sass": "^1.58.0",
		"svelte": "^3.55.1",
		"svelte-chartjs": "^3.1.2",
		"svelte-check": "^3.0.3",
		"svelte-dragdroplist": "^1.1.1",
		"svelte-preprocess": "^5.0.1",
		"svelte2tsx": "^0.6.1",
		"ts-results": "^3.3.0",
		"ts-results-es": "^3.5.0",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.1"
	},
	"type": "module",
	"cross-os": {
		"tauri": {
			"win32": ".\\bin\\tauri.exe",
			"linux": "bin/tauri"
		}
	}
}
