{
  "name": "@orbitale/compotes-app",
  "version": "1.0.0",
  "private": true,
  "license": "LGPL-3.0-or-later",
  "scripts": {
    "app": "concurrently --names \"front,back\" --raw \"vite dev\" \"yarn tauri dev\"",
    "dev": "vite dev",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "yarn --cwd tests/webdriverio run test",
    "postinstall": "yarn --cwd tests/webdriverio/"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.5",
    "@sveltejs/adapter-static": "1.0.0-next.34",
    "@sveltejs/kit": "^1.0.0-next.361",
    "@tauri-apps/api": "^1.0.2",
    "@tauri-apps/cli": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@zerodevx/svelte-toast": "^0.7.2",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.8.0",
    "concurrently": "^7.2.2",
    "cross-os": "^1.5.0",
    "date-fns": "^2.28.0",
    "date-picker-svelte": "^2.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "luxon": "^2.4.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.53.0",
    "svelte": "^3.49.0",
    "svelte-chartjs": "^1.1.4",
    "svelte-check": "^2.8.0",
    "svelte-dragdroplist": "^1.1.1",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.11",
    "ts-results": "^3.3.0",
    "ts-results-es": "^3.5.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.14"
  },
  "type": "module",
  "cross-os": {
    "tauri": {
      "win32": ".\\bin\\tauri.exe",
      "linux": "bin/tauri"
    }
  }
}
