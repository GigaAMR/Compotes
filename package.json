{
	"name": "@orbitale/compotes-app",
	"version": "1.0.0",
	"private": true,
	"license": "LGPL-3.0-or-later",
	"scripts": {
		"app": "concurrently --names \"front,back\" --raw \"vite dev\" \"yarn tauri dev\"",
		"dev": "vite dev",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. src && eslint --ignore-path .gitignore src",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. src",
		"test": "yarn --cwd tests/webdriverio run test",
		"postinstall": "yarn --cwd tests/webdriverio/"
	},
	"devDependencies": {
		"@popperjs/core": "^2.11.6",
		"@sveltejs/adapter-static": "1.0.0-next.48",
		"@sveltejs/kit": "^1.0.0-next.571",
		"@tauri-apps/api": "^1.2.0",
		"@tauri-apps/cli": "^1.2.1",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@zerodevx/svelte-toast": "^0.8.2",
		"bootstrap": "^5.2.3",
		"chart.js": "^4.0.1",
		"concurrently": "^7.6.0",
		"cross-os": "^1.5.0",
		"date-fns": "^2.29.3",
		"date-picker-svelte": "^2.2.4",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"luxon": "^3.1.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.56.1",
		"svelte": "^3.53.1",
		"svelte-chartjs": "^3.1.0",
		"svelte-check": "^2.10.0",
		"svelte-dragdroplist": "^1.1.1",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.21",
		"ts-results": "^3.3.0",
		"ts-results-es": "^3.5.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^3.2.5"
	},
	"type": "module",
	"cross-os": {
		"tauri": {
			"win32": ".\\bin\\tauri.exe",
			"linux": "bin/tauri"
		}
	}
}
